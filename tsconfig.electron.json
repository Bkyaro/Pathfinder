{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "outDir": "dist-electron",
      "allowJs": true,
      "skipLibCheck": true,
      "isolatedModules": true,
      "noEmit": false
    },
    "include": ["electron/**/*"]
  }